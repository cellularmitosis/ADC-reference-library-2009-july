// Change this to your short version string#define kBundleShortVersion __OUTPUT_FILENAME__ " version 1.3.0"// Change this to the string to display in the Finder's Get Info window#define kBundleGetInfo kBundleShortVersion ", © 2000-2005 Apple"plist{	dictionary	{		key "CFBundleName" value string __OUTPUT_FILENAME__		key "CFBundleGetInfoString" value string kBundleGetInfo		key "CFBundleShortVersionString" value string kBundleShortVersion				key "CFBundlePackageType" value string __OUTPUT_TYPE__		key "CFBundleSignature" value string __OUTPUT_CREATOR__		key "CFBundleExecutable" value string __OUTPUT_FILENAME__		key "CFBundleVersion" value string "1.4.0"				key "CFBundleDevelopmentRegion" value string "English"		key "CFBundleInfoDictionaryVersion" value string "6.0"	}}// Change this to be localized for Englishlocalize "English"{	comment "Localized versions of Info.plist keys"	key "CFBundleName" value string __OUTPUT_FILENAME__	key "CFBundleGetInfoString" value string kBundleGetInfo	key "CFBundleShortVersionString" value string kBundleShortVersion}