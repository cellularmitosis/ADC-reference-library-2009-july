:AStructMacs:"ProgStrucMacs.a	Asm "{MPW}Interfaces:AStructMacs:"ProgStrucMacs.aFlowCtlMacs.d  "{MPW}Interfaces:AStructMacs:"FlowCtlMacs.a	Asm "{MPW}Interfaces:AStructMacs:"FlowCtlMacs.aUMAFailure.a.o  UMAFailure.a ProgStrucMacs.d FlowCtlMacs.d	Asm {depDir}UMAFailure.a -o {targDir}UMAFailure.a.o {AOptions}