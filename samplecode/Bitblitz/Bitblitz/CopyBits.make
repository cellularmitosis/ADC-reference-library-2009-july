ToolDir		=	{ShellDirectory}BitBlitz:Source		= 	{ToolDir}Source:Resource	= 	{ToolDir}Resources:Headers		=	{ToolDir}LibHeaders:LibObject	=	{ToolDir}LibObject:Object		=	{ToolDir}ObjectCode:COptions 	= -r -n -b2 			  -d __PRECOMP__                -i {ShellDirectory}BitBlitz:LibHeaders:               -i {ShellDirectory}BitBlitz:Source:ToolObjs	= 		"{Object}"MainStuff.c.o    		"{Object}"BlendUtils.c.o   		"{Object}"OffscreenUtils.c.o   		"{Object}"BitBlitz.c.o "{Object}MainStuff.c.o" 		"{ToolDir}BitBlitz.make" 								"{Source}"MainStuff.c	C	{COptions} -o 			"{Object}MainStuff.c.o" 		"{Source}"MainStuff.c					"{Object}BitBlitz.c.o" 		"{ToolDir}BitBlitz.make" 								"{Source}"BitBlitz.c	C	{COptions} -o 			"{Object}BitBlitz.c.o" 			"{Source}"BitBlitz.c					"{Object}BlendUtils.c.o" 		"{ToolDir}BitBlitz.make" 								"{Source}"BlendUtils.c	C	{COptions} -o 			"{Object}BlendUtils.c.o" 		"{Source}"BlendUtils.c					"{Object}OffscreenUtils.c.o" 	"{ToolDir}BitBlitz.make" 								"{Source}"OffscreenUtils.c	C	{COptions} -o 			"{Object}OffscreenUtils.c.o" 	"{Source}"OffscreenUtils.c					BitBlitz	컴 {ToolObjs} "{ToolDir}BitBlitz.make"	Link -w -t APPL -c BLTZ -mf 		{ToolObjs} 		"{Libraries}"Interface.o 		"{Libraries}"Runtime.o 		"{CLibraries}"StdCLib.o 		"{LibObject}"Library.o 		"{LibObject}"myLib.o 		-o "{ToolDir}BitBlitz"	BitBlitz	컴   "{Resource}BitBlitz.r"    			"{ToolDir}BitBlitz.make"			Rez  "{Resource}BitBlitz.r" -o 			"{ToolDir}BitBlitz" -appendBitBlitz	컴   "{Resource}AlertDlogUtils.r"    	"{ToolDir}BitBlitz.make"			Rez  "{Resource}AlertDlogUtils.r" -o 	"{ToolDir}BitBlitz" -append