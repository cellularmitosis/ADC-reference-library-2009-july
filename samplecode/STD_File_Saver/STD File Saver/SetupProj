(deletemenu Build || set status 0) ³Dev:Null(deleteMenu Source || set status 0) ³Dev:Nulladdmenu Build "Build StdFileSaver/B" "echo ¶"¶" >{Worksheet}; (buildprogram StdFileSaver) ··{Worksheet};beep"addmenu Build (- ""addmenu Build "Delete Objects" "delete :Object:Å"addMenu Source '(:Source:Å.c' ""for fn in `files -t TEXT :Source:Å.c`	addmenu Source "	{fn}" "Open ¶":Source:{fn}¶""endaddmenu Source (- ""addMenu Source '(:Source:Å.h' ""for fn in `files -t TEXT :Source:Å.h`	addmenu Source "	{fn}" "Open ¶":Source:{fn}¶""endaddmenu Source (- ""addMenu Source '(:Source:Å.a' ""for fn in `files -t TEXT :Source:Å.a`	addmenu Source "	{fn}" "Open ¶":Source:{fn}¶""endaddmenu Source (- ""addMenu Source '(:Source:Å.r' ""for fn in `files -t TEXT :Source:Å.r`	addmenu Source "	{fn}" "Open ¶":Source:{fn}¶""endaddmenu Source (- ""addMenu Source (: ""for fn in `files -t TEXT`	addmenu Source "	{fn}" "Open ¶"{fn}¶""endecho "search '/////' :Source:Å.c" >{Worksheet}