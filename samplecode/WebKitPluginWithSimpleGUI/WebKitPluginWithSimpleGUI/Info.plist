<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>English</string>
	<key>CFBundleExecutable</key>
	<string>${EXECUTABLE_NAME}</string>
	<key>CFBundleName</key>
	<string>${PRODUCT_NAME}</string>
	<key>CFBundleIconFile</key>
	<string></string>
	<key>CFBundleIdentifier</key>
	<string>com.yourcompany.yourcocoabundle</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	
		<!-- Identify your bundle as a WebKit Plugin -->
	<key>CFBundlePackageType</key>
	<string>WBPL</string>
	
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>1.0</string>
	
		<!-- Identify the principle class implementing the WebPlugInViewFactory protocol -->
	<key>NSPrincipalClass</key>
	<string>PluginWithSimpleGUI</string>
	
		<!-- provide a user readable name and description for your plug-in -->
	<key>WebPluginName</key>
	<string>Simple GUI plug-in</string>
	<key>WebPluginDescription</key>
	<string>This is a sample WebKit plug-in that shows how to use a GUI designed in Interface Builder.</string>

		<!-- identify the mime types your plug-in is capable of displaying -->
	<key>WebPluginMIMETypes</key>
	<dict>
		<key>application/x-simpleguiplugin</key>
		<dict>
			<key>WebPluginExtensions</key>
			<array>
				<string>sgui</string>
			</array>
			<key>WebPluginTypeDescription</key>
			<string>Simple GUI Plug-in Data</string>
		</dict>
	</dict>
</dict>
</plist>
