lue string __OUTPUT_TYPE__		key "CFBundleSignature" value string __OUTPUT_CREATOR__		key "CFBundleExecutable" value string __OUTPUT_FILENAME__				key "CFBundleDevelopmentRegion" value string "English"		key "CFBundleInfoDictionaryVersion" value string "6.0"		key "CSResourcesFileMapped" value boolean true		key "LSRequiresCarbon" value boolean true		key "NSAppleScriptEnabled" value boolean false				key "CFBundleDocumentTypes" value array		[			dictionary			{				key "CFBundleTypeName" value string "QuickTime Document"				//key "CFBundleTypeIconFile" value string "129"				key "CFBundleTypeRole" value string "Viewer"				key "CFBundleTypeOSTypes" value array				[					string '****'				]				key "CFBundleTypeExtensions" value array				[					string '*'				]			}		]	}}// Change this to be localized for Englishlocalize "English"{	comment "Localized versions of Info.plist keys"	key "CFBundleName" value string __OUTPUT_FILENAME__	key "CFBundleGetInfoString" value string kBundleGetInfo	key "CFBundleShortVersionString" value string kBundleShortVersion}