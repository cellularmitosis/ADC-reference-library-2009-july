 ÄÄ {Resources} {PPCObjects}	PPCLink -o "{TargetApp}" -m main ¶		{PPCObjects} ¶		{SharedLibraries}CarbonLib# bundle"{TargetApp}" ÄÄ {Resources} {PPCObjects}	SetFile -t 'APPL' -c '????' -a ib "{TargetApp}"# source code:obj:HTMLUserPane.c.ppc Ä  HTMLUserPane.c ¶		HTMLUserPane.h HTMLControl.h debugf.h :obj:make.com	{CCppc} HTMLUserPane.c -o :obj:HTMLUserPane.c.ppc:obj:HTMLControl.c.ppc Ä  HTMLControl.c HTMLControl.h History.h :obj:make.com	{CCppc} HTMLControl.c -o :obj:HTMLControl.c.ppc### objects folder:obj:make.com Ä MakeFile	( NewFolder obj || set status 0 ) · dev:null	Echo "HTMLUserPane 1.0" > :obj:make.com